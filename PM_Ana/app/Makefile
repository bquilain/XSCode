$(TARGET): $(OBJS)
	@echo "Now make $@"
	@$(CPP) -o $@ $(OBJS) $(CXXFLAGS) $(LIBS) -I $(LIB) -I $(SRC) -I $(INC)
	@echo "..Compile done! "

.cc.o:
	@echo "Start Compiling $<"
	@$(CPP) -c $(CXXFLAGS) $(GDBFLAGS) $< -I $(LIB) -I $(SRC) -I $(INC)
	@echo ".. Compiling Object Files $<   --> done"
	@echo ""

clean: 
	@echo "Now Clean Up"
	rm -f $(TARGET) *~ *.o *.o~ core



